 '(ps-n-up-printing 2)
 '(ps-printer-name "")
 '(ps-printer-name-option "-P")
 '(query-user-mail-address nil)
 '(scroll-bar-mode nil)
 '(scroll-conservatively 100)
 '(scroll-margin 2)
 '(show-paren-mode t)
 '(tab-width 4)
 '(tool-bar-mode nil)
 '(transient-mark-mode t)
 '(vc-diff-switches (quote ("-U500"))))

